extends ../layout

block content
    div(
        class = "ui container segment"
    )
        h1(
            class = "ui centered header"
        ) Unconfirmed Properties
        table(
            class = "ui celled striped sortable table"
        )
            tr()
                th(
                    align = "center"
                ) Name
                th(
                    align = "center"
                ) Address
                th(
                    align = "center"
                ) City
                th(
                    align = "center"
                ) Zip
                th(
                    align = "center"
                ) Size
                th(
                    align = "center"
                ) Type
                th(
                    align = "center"
                ) Public
                th(
                    align = "center"
                ) Commercial
                th(
                    align = "center"
                ) ID
                th(
                    align = "center"
                ) Owner
            each result in results
                li() #{result.msg}
